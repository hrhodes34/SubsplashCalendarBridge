# Subsplash Calendar Bridge Configuration
# Copy this file to .env and modify as needed

# Google Calendar Configuration
GOOGLE_CALENDAR_ID=your_calendar_id_here
SUBSPLASH_EMBED_URL=https://subsplash.com/+wrmm/lb/ca/+pysr4r6?embed

# Debug and File Saving Options (for automated systems)
# Set these to 'true' only when you need debugging or status files

# Save debug HTML files (default: false for production)
SAVE_DEBUG_FILES=false

# Enable verbose page structure analysis (default: false for production)
VERBOSE_DEBUG=false

# Save sync status files (default: false for production)
SAVE_STATUS_FILE=false

# Logging level (info, warning, error)
LOG_LEVEL=info
